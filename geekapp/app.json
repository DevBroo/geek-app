{
  "expo": {
    "name": "geeklappy-app",
    "slug": "geekapp-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "D:/GeekLappy/geeklappylogo.png",
    "scheme": "geekapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "D:/GeekLappy/geeklappylogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "androidStatusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#FAFAFA",
        "translucent": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "D:/GeekLappy/geeklappylogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "D:/GeekLappy/geeklappylogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
